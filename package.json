{
  "name": "wasm-memx",
  "version": "1.0.0",
  "description": "High-performance shared memory manager with WebAssembly backend.",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "dev": "pnpm run build:dev",
    "build": "rimraf dist && rollup --config",
    "watch": "rimraf dist && rollup --config --watch",
    "build:watch": "rollup -c -w",
    "build:dev": "rollup -c --environment NODE_ENV:development",
    "build:prod": "pnpm run build:wasm && rollup -c --environment NODE_ENV:production",
    "build:dev:watch": "rollup -c --environment NODE_ENV:development -w",
    "build:prod:watch": "rollup -c --environment NODE_ENV:production -w",
    "docs": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build:wasm": "cd rust && cargo build --target wasm32-unknown-unknown --release",
    "build:wasm:web": "cd rust && cargo build --target wasm32-unknown-unknown --release"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "wasm",
    "webassembly",
    "sharedarraybuffer",
    "high-performance",
    "memory"
  ],
  "author": "Frank",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.1.4",
    "@rollup/plugin-url": "^8.0.2",
    "@rollup/plugin-wasm": "^6.2.2",
    "@types/node": "^24.1.0",
    "@wasm-tool/rollup-plugin-rust": "^3.0.5",
    "binaryen": "^123.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.46.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.8",
    "typescript": "^5.9.2",
    "vitepress": "^1.6.3"
  },
  "dependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "rollup-plugin-serve": "^3.0.0"
  }
}
